input[type="checkbox"],input[type="radio"]{
    appearance: none;
    -webkit-appearance: none;
    background-color: #fff;
    border: 1px solid #bcbcbc;
    padding: 5px;
    display: inline-block;
    position: relative;
} 
input[type="checkbox"]{
    border-radius: 3px;
}
input[type="checkbox"]:checked,input[type="radio"]:checked,input[type="radio"]:checked:after{
    background-color: #2680cd;
    border: 1px solid #2680cd;
}
input[type="checkbox"]:checked:after{
    content: "\2713";
    position: absolute;
    top: -6px;
    left:1px;
