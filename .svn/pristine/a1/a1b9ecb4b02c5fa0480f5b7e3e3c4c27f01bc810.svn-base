.op-wrapper.records{
	position: absolute;
    left: 0;
    right: 0;
    top: 82px;
    bottom: 10px;
    padding: 0 9px;
    order: 2;
    display: flex;
    flex: 1;
}

.record-mic{
	width: 97px;
	height: 99%;
	padding: 3px;
	font-size: 12px;
	color: #000;
	background-color:#fff; 
    overflow-x:hidden;
    overflow-y:auto;
    order: 1;
    -webkit-transition: width 0.5s;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    align-content: space-around;
}
.record-mic.column{
	width: 52px;
	flex-direction: column;
	flex-wrap: nowrap;
	align-items: center;
	justify-content: space-around;
}
.record-mic.right-close{
	width: 0;
	padding: 0;
}
.record-mic.right-close .mic-item{
	width: 0;
}
.records .left-section{
	position: relative;
    background: #fff;
    height: 100%;
    margin:0 12px;
    flex: 1;
    order: 2;
    -webkit-transition: width 0.5s;
}

.records .right-section{
	position: relative;
	background: #fff;
	width: 209px;
	order: 3;
	overflow-y:auto;
	-webkit-transition: width 0.5s;
}

.records .right-section.right-close{
	width: 0;
}
.record-mic-open,
.right-section-open{
	position: absolute;
	height: 78px;
	width: 9px;
	top: 50%;
	margin-right: -3px;
    margin-top: -90px;
    z-index: 10;
}
.record-mic-open.column{
	left: 68px;
}
.record-mic-open{
	left: 113px;
	-webkit-transition: left 0.5s;
}
.right-section-open{
	right: 222px;
	-webkit-transition: right 0.5s;
}
.record-mic-open-icon,
.right-section-open-icon{
	width: 100%;
    height: 17px;
    position: relative;
    top: 50%;
    margin-top: -5px;
	background-image: url(../images/right-close.png);
    background-repeat: no-repeat;
    background-position: 50% 50%;
    -webkit-transition: -webkit-transform 0.5s;
}
.right-section-open-icon{
	-webkit-transform: rotate(0deg);
}
.record-mic-open-icon{
	-webkit-transform: rotate(180deg);
}
.record-mic-open.right-close{
	left: 8px;
}
.record-mic-open.right-close .record-mic-open-icon{
	-webkit-transform: rotate(0deg);
}
.right-section-open.right-close{
	right: 8px;
}
.right-section-open.right-close .right-section-open-icon{
	-webkit-transform: rotate(180deg);
}


